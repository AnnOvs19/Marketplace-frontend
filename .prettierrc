{
  "tabWidth": 2,
  "semi": true,
  "printWidth": 80,
  "useTabs": false,
  "trailingComma": "none",
  "importOrder": [
    "^@react/(.*)$",
    "^@utils/(.*)$",
    "^@api/(.*)$",
    "^@hooks/(.*)$",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "^assets/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
